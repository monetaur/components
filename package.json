  "module": "lib/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@testing-library/react": "^11.0.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "semantic-release": "^17.1.1",
    "storybook": "^7.5.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "scripts": {
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build": "npm run build:es6 && npm run build:cjs",
    "build:es6": "BABEL_ENV=es6 babel --ignore '**/*.test.jsx,**/*.stories.jsx' -d lib/ src/",
    "build:cjs": "webpack --mode production",
    "build-storybook": "storybook build",
    "clean": "rm -rf lib && rm -rf dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monetaur/components.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monetaur/components/issues"
  },
  "homepage": "https://github.com/monetaur/components#readme",
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.3",
    "@monetaur/hooks": "^1.1.3",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "styled-components": "^5.0.0-beta.9"
  },
  "files": [
    "dist",
    "lib",
    "themes"
